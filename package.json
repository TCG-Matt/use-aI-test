{
  "name": "ai-poc-template",
  "version": "1.0.0",
  "description": "A Git template for AI Proof of Concepts with strict guard rails and best practices",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest",
    "lint": "next lint",
    "format": "prettier --write .",
    "build": "next build",
    "verify": "npm run type-check && npm run lint && npm run test",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "scaffold": "node scripts/scaffold.js",
    "dev": "next dev",
    "start": "next start",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose build"
  },
  "keywords": [
    "ai",
    "template",
    "poc",
    "typescript"
  ],
  "engines": {
    "node": ">=24.12.0"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
